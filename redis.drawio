        <mxCell id="Ob7KSYr4x1QFicvUf9MM-8" value="快速列表" style="text;html=1;strokeColor=#d6b656;fillColor=#fff2cc;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontSize=12;shadow=1;" vertex="1" parent="R8Xmmlt4IPB7zrE09Ldw-1">
