        <mxCell id="BsA3-4Pb5x5gcWoGTt1B-8" value="数据库性能抖动" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;shadow=1;" parent="BsA3-4Pb5x5gcWoGTt1B-1" vertex="1">
